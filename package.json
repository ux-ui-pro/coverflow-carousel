{
  "name": "coverflow-carousel",
  "version": "0.3.0",
  "description": "Tiny coverflow carousel Web Component. Exposes API via attributes + events (explicit element registration).",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/coverflow-carousel.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/coverflow-carousel/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/coverflow-carousel",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "dev": "vite",
    "verify": "yarn lint && yarn typecheck",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./coverflow-carousel.css": "./dist/coverflow-carousel.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/node": "25.0.3",
    "lightningcss": "1.30.2",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4"
  },
  "keywords": [
    "typescript",
    "frontend",
    "library",
    "carousel",
    "coverflow",
    "web component",
    "web-component",
    "custom elements",
    "custom-elements",
    "shadow dom",
    "shadow-dom",
    "ui",
    "component",
    "accessibility",
    "a11y",
    "coverflow-carousel"
  ]
}
